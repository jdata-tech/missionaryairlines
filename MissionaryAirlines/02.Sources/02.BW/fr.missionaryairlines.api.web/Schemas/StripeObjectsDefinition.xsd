					</sequence>
				</complexType>
			</element>
			<element name="receiver" minOccurs="0">
				<complexType>
					<sequence>
						<element name="address" type="tns:AddressType" minOccurs="0"/>
						<element name="amount_charged" type="int" minOccurs="0"/>
